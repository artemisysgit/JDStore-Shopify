                  <div class="tp-shop-widget-filter">
                    <div
                      id="slider-range"
                      class="mb-10 ui-slider ui-corner-all ui-slider-horizontal ui-widget ui-widget-content"
                    >
                      <div class="ui-slider-range ui-corner-all ui-widget-header" style="width: 0%; left: 60%"></div>
                      <span
                        tabindex="0"
                        class="ui-slider-handle ui-corner-all ui-state-default"
                        style="left: 60%"
                      ></span>
                      <span
                        tabindex="0"
                        class="ui-slider-handle ui-corner-all ui-state-default"
                        style="left: 60%"
                      ></span>
                    </div>
                    <div class="tp-shop-widget-filter-info d-flex align-items-center justify-content-between">
                      <span class="input-range">
                        <input type="text" id="amount">
                      </span>
                      <button class="tp-shop-widget-filter-btn" type="submit">Filter</button>
                    </div>
                  </div>
                </form>
              </div>
            </div>
            <div class="tp-shop-widget mb-50">
              <h3 class="tp-shop-widget-title">Product Status</h3>

              <div class="tp-shop-widget-content">
                <div class="tp-shop-widget-checkbox">
                  <ul class="filter-items filter-checkbox">
                    <form class="my-form1" action="{{ collection.handle }}" id="stock" method="get">
                      <li class="filter-item checkbox">
                        <input id="in_stock" type="checkbox" value="1" name="filter.v.availability">
                        <label for="in_stock">In Stock</label>
                      </li>
                    </form>
                    <li class="filter-item checkbox">
                      <input disabled id="on_sale" type="checkbox" value="0">
                      <label for="on_sale" style="color:#EBEBE4">Out Of Stock</label>
